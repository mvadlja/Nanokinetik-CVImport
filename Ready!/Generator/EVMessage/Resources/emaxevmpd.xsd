												<xs:whiteSpace value="collapse"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="term" id="AS.T.2">
										<xs:annotation>
											<xs:documentation>The translation of the substance name must be specified.
Business Rules: AS.T.2.BR.</xs:documentation>
										</xs:annotation>
