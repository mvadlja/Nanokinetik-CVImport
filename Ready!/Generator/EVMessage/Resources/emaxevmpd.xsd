			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:enumeration value="1"/>
					<xs:enumeration value="2"/>
					<xs:enumeration value="4"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="virtual" type="xs:int" use="optional" id="AS..2">
			<xs:annotation>
				<xs:documentation>Reserved for EMA Use only
